<?xml version="1.0"?>
<config>
	<event name="view_block_abstract_to_html_before">
		<observer name="commercebug_for_graph" instance="Teninteractivellc_Commercebug_Model_Observer_Dot" method="before" />
        <observer name="commercebug_collect_blocksandlayout" instance="Teninteractivellc_Commercebug_Model_Collectorblocksandlayout" method="collectInformation" />		
	</event>
	<event name="core_block_abstract_to_html_after">
		<observer name="for_graph" instance="Teninteractivellc_Commercebug_Model_Observer_Dot" method="after" />
	</event>
	<event name="core_layout_render_element">
		<observer name="commercebug_for_mage2_graph" instance="Teninteractivellc_Commercebug_Model_Observer_Dot" method="afterElement" />
	</event>
	<event name="controller_action_predispatch">
		<observer name="commercebug_collect_controller" instance="Teninteractivellc_Commercebug_Model_Collectorcontroller" method="collectInformation" />
		<observer name="commercebug_collect_systeminfo" instance="Teninteractivellc_Commercebug_Model_Collectorsysteminfo" method="collectInformation" />
        <observer name="ascommercebug_jiggerconfig" instance="Teninteractivellc_Commercebug_Model_Observer" method="jiggerConfig" />		
	</event>
	<event name="model_load_after">
		<observer name="commercebug_collect_models" instance="Teninteractivellc_Commercebug_Model_Collectormodels" method="collectInformation" />
	</event>
	<event name="eav_collection_abstract_load_before">
		<observer name="commercebug_collect_model_eavcollections" instance="Teninteractivellc_Commercebug_Model_Collectorcollections" method="collectInformation" />
	</event>
	<event name="core_collection_abstract_load_after">
		<observer name="commercebug_collect_model_collections" instance="Teninteractivellc_Commercebug_Model_Collectorcollections" method="collectInformation" />
	</event>
	<event name="controller_front_send_response_before">
		<observer name="commercebug_add_commercebug_init" instance="Teninteractivellc_Commercebug_Model_Observer" method="addCommercebugInit" />
	</event>
	<event name="controller_action_postdispatch">
		<observer name="commercebug_model_layoutviewer_observer" instance="Teninteractivellc_Commercebug_Model_Layoutviewer_Observer" method="checkForLayoutDisplayRequest" />
	</event>
